@model IEnumerable<BooksTracking.Areas.Identity.Data.ApplicationUser>
@{
    ViewData["Title"] = "Manage Users";
}

<h1 class="fancy-title">Users</h1>

<table class="table table-striped">
    <thead>
        <tr>
            <th>Email</th>
            <th>First Name</th>
            <th>Last Name</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.Email</td>
                <td>@user.FirstName</td>
                <td>@user.LastName</td>
            </tr>
        }
    </tbody>
</table>
